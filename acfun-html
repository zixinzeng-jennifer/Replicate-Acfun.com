 function post()
 {
	 var query=document.getElementById('search-text').value;
	 document.form1.action="https://www.acfun.cn/search/?#query="+query+";cid=0;sort=1;sid=0"
	 document.form1.submit();
 }
 function hide(id)
 {
	 var a=document.getElementById(id);
	 a.style.display="none";
 }
 var movex;
 var movey;
 function mousemove(e){
 	e=e || window.event;
 	if (e.pageX || e.pageY)
 	{
 		movex=e.pageX;
 		movey=e.pageY;
 	}
 	
 }
 
 window.onscroll = function() {
	 var y=document.getElementById('navigate');
	 	var z=document.getElementById('pos');
	 	var a=document.getElementById('AC_drop');
		 var b=document.getElementById('bangumi_drop');
		 
		 var c=document.getElementById('anime_drop');
		 
		 var d=document.getElementById('ent_drop');
		 
		 var e=document.getElementById('life_drop');
		
		 var f=document.getElementById('music_drop');
		
		 var g=document.getElementById('dance_drop');
		
		 var h=document.getElementById('game_drop');
		
		 var i=document.getElementById('tech_drop');
		 
		 var j=document.getElementById('movi_drop');
		 
		 var k=document.getElementById('sport_drop');
		 
		 var l=document.getElementById('yt_drop');
		 
		 var m=document.getElementById('wen_drop');
		 var place=y.getBoundingClientRect();
		 var secondplace=z.getBoundingClientRect();
		 //alert(secondplace.top);
		if(place.top<90)
		{
			//alert('调整');
			y.style.position="fixed";
			y.style.top="70px";
			y.style.zIndex="300";
			y.style.backgroundColor="#e8e8e8";
			a.style.position="fixed";
			a.style.top="134px";
			a.zIndex="300";
			b.style.position="fixed";
			b.style.top="134px";
			b.zIndex="300";
			c.style.position="fixed";
			c.style.top="134px";
			c.zIndex="300";
			d.style.position="fixed";
			d.style.top="134px";
			d.zIndex="300";
			e.style.position="fixed";
			e.style.top="134px";
			e.zIndex="300";
			f.style.position="fixed";
			f.style.top="134px";
			f.zIndex="300";
			g.style.position="fixed";
			g.style.top="134px";
			g.zIndex="300";
			h.style.position="fixed";
			h.style.top="134px";
			h.zIndex="300";
			i.style.position="fixed";
			i.style.top="134px";
			i.zIndex="300";
			j.style.position="fixed";
			j.style.top="134px";
			j.zIndex="300";
			k.style.position="fixed";
			k.style.top="134px";
			k.zIndex="300";
			l.style.position="fixed";
			l.style.top="134px";
			l.zIndex="300";
			m.style.position="fixed";
			m.style.top="134px";
			m.zIndex="300";
			//alert(secondplace.top);
		}
		if(secondplace.top>140)
		{
			//alert('调整')
			y.style.position="absolute";
			y.style.top="250px";
			y.style.backgroundColor="#fff";
			a.style.position="absolute";
			a.style.top="300px";
			b.style.position="absolute";
			b.style.top="300px";
			c.style.position="absolute";
			c.style.top="300px";
			d.style.position="absolute";
			d.style.top="300px";
			e.style.position="absolute";
			e.style.top="300px";
			f.style.position="absolute";
			f.style.top="300px";
			g.style.position="absolute";
			g.style.top="300px";
			h.style.position="absolute";
			h.style.top="300px";
			i.style.position="absolute";
			i.style.top="300px";
			j.style.position="absolute";
			j.style.top="300px";
			k.style.position="absolute";
			k.style.top="300px";
			l.style.position="absolute";
			l.style.top="300px";
			m.style.position="absolute";
			m.style.top="300px";
			
		}
	 
 }
 function showwz(id)
 {
	 document.getElementById('wz1').style.display="none";
	 document.getElementById('wz2').style.display="none";
	 document.getElementById('wz3').style.display="none";
	 document.getElementById(id).style.display="block";
	 
	 
 }
 window.onload = function(){
	 	var i=0;
	    var sld = {
	        init: function() {
	        	//alert('开始了');
	        	
	            var a=document.getElementById('slider');
	            var b=document.getElementById('slider-link');
	            var c=document.getElementById('intro');
	            
	            var pics=new Array();
	            var links=new Array();
	            var title=new Array();
	            pics[0]="https://imgs.aixifan.com/3LvlkB5eGL-MRjaQr-EvmUra-IVr2Un-BzQNRj.jpg?imageView2/1/w/508/h/260/q/100";
	            links[0]="https://www.acfun.cn/bangumi/ab6000307";
	            title[0]="索斯机兽ZERO:向天怒号";
	            pics[1]="https://imgs.aixifan.com/t2xWJmBziF-y63qae-eeyuiy-uYrUzy-ZZRfmi.jpg?imageView2/1/w/508/h/260/q/100";
	            links[1]="https://www.acfun.cn/v/ac11842957";
	            title[1]="【墨菲】《冰血暴》第2期：Fargo集团登场！";
	            pics[2]="https://imgs.aixifan.com/0MjfTVlQ15-veyEBj-uaMbuu-ja2umy-BFNvY3.jpg?imageView2/1/w/508/h/260/q/100";
	            links[2]="https://www.acfun.cn/spn/zzx";
	            title[2]="猴有才：漫改影视系列大盘点（二）";
	            pics[3]="https://imgs.aixifan.com/aiGUyNVMZF-miIBne-Bjq2qm-RzqMNz-RJn2Yb.jpg?imageView2/1/w/508/h/260/q/100";
	            links[3]="https://www.acfun.cn/bangumi/ab6000526";
	            title[3]="加班的那点事儿";
	            pics[4]="https://imgs.aixifan.com/IN9BHTQThz-E3aayy-Ufmiyq-7JFBF3-QBrEN3.jpg?imageView2/1/w/508/h/260/q/100";
	            links[4]="https://www.acfun.cn/spn/ysnd";
	            title[4]="脑洞改变世界？点击围观！";
	            
	            var name;
	            var name2;
	            a.src=pics[0];
	            b.href=links[0];
	            c.innerHTML=title[0];
	            //alert('初始化了');
	            var sld = setInterval(function(){
	           		//alert(i);
	           		
	                name="cir"+String(i%5);
		            name2="cir"+String((i+1)%5);
	           		//alert(name);
	           		//alert(name2);
	           		var d=document.getElementById(name);
		            var e=document.getElementById(name2);
	           		d.src="circle.jpg";
	           		e.src="circle2.jpg";
	           		//alert('!');
	           		i=i+1;
	                i=i%5;
	                a.src=pics[i];
	                b.href=links[i];
	                c.innerHTML=title[i];
	            }, 3000);
	            
	        }     
	    }
	    sld.init();
		setup();
		
}
 </script>
</head>
