 function setup()
 {
	 var a=document.getElementById('search-text');
	 var b=document.getElementById('hot');
	 b.style.left=(a.offsetLeft-20)+"px";
	 b.style.width=(a.clientWidth-10)+"px";
	 var d=document.getElementById('download');
	 var e=document.getElementById('dl');
	 d.style.left=(e.offsetLeft-50)+"px";
	 var e=document.getElementById('history');
	 var f=document.getElementById('his');
	 e.style.left=(f.offsetLeft-50)+"px";
	 document.getElementById('btn').style.left=e.style.left;
	 var g=document.getElementById('upl');
	 document.getElementById('upload').style.left=(g.offsetLeft-100)+"px";
	 document.getElementById('upload').style.width=(g.clientWidth*4)+"px";
	 //alert(a.offsetLeft);
	 //alert(a.offsetLeft);
	 //alert(b.style.left);
	 
 }
 
 
